<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.gaussic.model.ULogEntity" table="u_log" schema="wind">
        <id name="id" column="id"/>
        <property name="lUserId" column="l_user_id"/>
        <property name="lLogTime" column="l_log_time"/>
        <property name="lLoginOrlogOut" column="l_loginOrlogOut"/>
        <property name="lNote" column="l_note"/>
    </class>
</hibernate-mapping>